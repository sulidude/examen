<h1>TodoList#new</h1>
<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="todolist new">
  <h2>Create Todo List</h2>

  <%= form_for(:todolist, :url => {:action => 'create'}) do |f| %>

    <table summary="todolist form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
    </table>

    <div class="form-buttons">
      <%= submit_tag("Create todolist") %>
    </div>

  <% end %>
</div>
